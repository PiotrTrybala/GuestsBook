generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

model AdminUsers {
    id Int @id @default(autoincrement())
    username String
    password String
}

model Guests {
    id Int @id @default(autoincrement())
    first_name String
    last_name String
    description String
    creation_date DateTime @db.Timestamp()
}